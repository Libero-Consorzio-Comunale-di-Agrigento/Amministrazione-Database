<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <property name="versione"/>
    <changeSet author="snegroni" id="loadData-oggetti-standard-${versione}" runOnChange="true" runAlways="true">
        <comment>SELECT OBJECT_NAME, OBJECT_TYPE FROM user_objects where object_type in ('FUNCTION','PACKAGE', 'PACKAGE BODY', 'PROCEDURE', 'TRIGGER','VIEW','SYNONYM');
        </comment>
        <sql dbms="oracle"
             endDelimiter=";"
             splitStatements="true"
             stripComments="true">
            truncate table OGGETTI_STANDARD;
        </sql>
        <loadData
                relativeToChangelogFile="true"
                  file="csv/oggetti_standard.csv"
                  separator=";"
                  tableName="OGGETTI_STANDARD">
            <column name="OBJECT_NAME" type="STRING"/>
            <column name="OBJECT_TYPE" type="STRING"/>
        </loadData>
    </changeSet>
</databaseChangeLog>